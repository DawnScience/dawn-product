<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="dViewer"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="dViewer">
         <property
               name="appName"
               value="dViewer">
         </property>
         <property
               name="startupForegroundColor"
               value="FFFFFF">
         </property>
         <property
               name="startupMessageRect"
               value="10,10,200,20">
         </property>
         <property
               name="startupProgressRect"
               value="0,513,1224,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="windowImages"
               value="icons/dViewer-16x16-icon.png,icons/dViewer-48x48-icon.png">
         </property>
         <property
               name="aboutImage"
               value="icons/dViewer-48x48-icon.png">
         </property>
         <property
               name="introBrandingImage"
               value="product:icons/dViewer-3Col-Splash.png">
         </property>
         <property
               name="introTitle"
               value="Welcome to dViewer">
         </property>
         <property
               name="aboutText"
               value="Welcome to dViewer product of DAWN collaboration, the tools set for online data analysis&#x0A;Copyright, 2010 - 2012 Diamond Light Source and ESRF&#x0A;&#x0A;Version 1.1 Beta&#x0A;&#x0A;This version of dViewer is licensed under&#x0A;the Eclipse Public License version 1.0.&#x0A;&#x0A;Please see the license file in the installation directory and &#x0A;contact us if you cannot locate it. More details can be found at&#x0A;http://www.dawnsci.org&#x0A;&#x0A;Based on Eclipse, www.eclipse.org &#x0A;Uses: Java, GEF, Log4J, Passerelle&#x0A;&#x0A;Diamond Light Source and ESRF are registered trade marks &#x0A;All other trademarks are the property of their respective owners.">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.installationPages">
      <page
            class="org.dawnsci.product.plugin.about.ReleaseNotesPage"
            id="org.dawnsci.product.plugin.about.releasenotespage"
            name="Release Notes">
      </page>
   </extension>
</plugin>
